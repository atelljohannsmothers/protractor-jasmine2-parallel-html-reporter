{
  "env": {
    "production": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "4"
            },
            "modules": false
          }
        ]
      ],
      "plugins": ["external-helpers", "transform-runtime"]
    },
    "test": {
      "presets": ["@ava/stage-4"],
      "plugins": [
        "istanbul",
        ["babel-plugin-aliases", [{
          "path": "/lib",
          "alias": "lib"
        }, {
          "path": "/test/helpers/fixtures",
          "alias": "fixtures"
        }, {
          "path": "/test/helpers/mocks",
          "alias": "mocks"
        }, {
          "path": "/test/helpers/spies",
          "alias": "spies"
        }, {
          "path": "/test/helpers/stubs",
          "alias": "stubs"
        }]]
      ]
    }
  }
}
